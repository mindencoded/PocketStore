<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="BaseUri" value="https://+:444/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="AuthenticationModes" value="OAUTH" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Simple.TraceLoggerFactoryAdapter, Common.Logging"
                      xdt:Transform="SetAttributes">
        <!-- level [ Trace | Debug | Info | Warn | Error | Fatal ] -->
        <arg key="level" value="Error" xdt:Transform="Replace" xdt:Locator="Match(key)" />
      </factoryAdapter>
    </logging>
  </common>
</configuration>